{
  "name": "Will It Rain?",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension will notify you every time if it will rain tomorrow. Never be left without an umbrelle again!",
  "homepage_url": "http://linkevin.me",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "notifications"
  ]
}